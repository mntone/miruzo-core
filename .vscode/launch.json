{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "FastAPI debug",
			"type": "debugpy",
			"cwd": "${workspaceFolder}",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"app.main:app",
				"--env-file",
				".env.development",
				"--host",
				"0.0.0.0",
				"--port",
				"1024",
				"--reload"
			],
			"jinja": true
		},
		{
			"name": "score_decay cli debug",
			"type": "debugpy",
			"cwd": "${workspaceFolder}",
			"request": "launch",
			"module": "scripts.score_decay",
			"jinja": true
		},
		{
			"name": "gataku-import debug",
			"type": "debugpy",
			"cwd": "${workspaceFolder}",
			"request": "launch",
			"module": "importers.import",
			"args": [
				"--limit",
				"50",
				"--report-variants"
			],
			"jinja": true
		}
	]
}
